n,m=map(int, input().split()) 
temp=[int(n) for n in input().split()]
a=sorted(temp)   #lưu mảng thời gian cv đã được sx
d=len(a)-1
b=[]  # tính tổng thời gian cho từng máy
c=[]  # lưu các vị trí
for i in range(m):
	c.append([])
for i in range(m):
	b.append(0)
while 1 and d>=0:
	i=0
	while i<m and d>=0:
		c[i].append(a[d])
		b[i]=b[i]+a[d]
		d=d-1
		i=i+1
	i=m-1
	while i>=0 and d>=0:
		c[i].append(a[d])
		b[i]=b[i]+a[d]
		d=d-1
		i=i-1
for i in temp:
	for j in range(m):
		if i in c[j]:
			print(j,end=" ")
			break
