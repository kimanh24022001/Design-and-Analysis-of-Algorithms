#include <iostream>
#include <math.h>
using namespace std;
int n, s[200];
bool isTrue(int s[200]){
	if ((s[1]==1) && (s[2*n]==0)){
		int count1=0,count0=0;
		for (int i=1;i<=2*n;i++){
			if (s[i]==1){
				count1+=1;
			}
			if (s[i]==0){
				count0+=1;
			}
			if (count0>count1){
				return 0;
			}
		}
		if (count0==count1){
			return 1;
		}
	}
	return 0;
}
void parentheses(int i){
	for (int j=1;j>=0;j--){
		s[i]=j;
		if (i==2*n){
			if (isTrue(s)==1){
			    for (int m=1;m<=i;m++){
			    	if (s[m]==1){
			    		cout<<"(";
			    	}
			    	else{
			    		cout<<")";
			    	}
			 
			    }
				cout<<"\n";
			}
		}
		else{
			parentheses(i+1);
			}
	}
}
int main() {
	cin>>n;
	parentheses(1);

	return 0;
}
