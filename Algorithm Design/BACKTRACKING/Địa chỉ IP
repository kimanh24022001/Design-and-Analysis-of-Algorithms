#include <iostream>
#include <math.h>
#include <string.h>

using namespace std;
int n,beg[100],fin[100],count=0;
char String[100];

void ipAddress(int i,int k){
	for ( int t=3;t>=1;t--){
		    int j=k+1;
		    int m=0;
		    int x=0;
            for (int h=j+t-1;h>=j;h--){
	         	x+=(int(String[h])-48)*pow(10,m);
      		m++;
          	}
          	if ((String[j]=='0') && (t>1)){
          		x=256;
          	}
            if ((j+t-1<=n-1) &&(x<=255) && (x>=0)) {
               beg[i]=j;
               fin[i]=j+t-1;

		       if ((j+t-1==n-1) && (i==3))  {
		       	  	for (int m=0;m<=i;m++){
		       	    		for (int h=beg[m];h<=fin[m];h++){
		       	                  cout<<String[h];
		       	    		}
		       	    		if (m!=i){
		       	    		   cout<<".";
		       	    		}
		       	    	}
		       	    	 cout<<"\n";
		       	    }
		      
	        	else{
		        	ipAddress(i+1,j+t-1);
		        	}
               }
	   
	}
}
int main() {
	cin>>String;
	n=strlen(String);
	ipAddress(0,-1);
	return 0;
}
